{
  "context_servers": {
    "browser-tools-context-server": {
      "source": "extension",
      "enabled": true,
      "settings": {}
    }
  },
  "toolbar": {
    "quick_actions": false
  },
  "show_edit_predictions": true,
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "terminal": {
    "shell": "system",
    "env": {
      "ZED_TERMINAL": "1"
    },
    "line_height": { "custom": 1.25 }
  },
  "project_panel": {
    "dock": "left",
    "indent_size": 12
  },
  "agent": {
    "always_allow_tool_actions": true,
    "inline_assistant_model": {
      "provider": "copilot_chat",
      "model": "gpt-5"
    },
    "model_parameters": [],
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-5"
    }
  },
  "vim_mode": true,
  "vim": {
    "toggle_relative_line_numbers": true
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "agent_font_size": 16,
  "ui_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "theme": {
    "mode": "system",
    "light": "One Dark",
    "dark": "One Dark"
  },
  "diagnostics": {
    "inline": {
      "enabled": true
    }
  }
}
